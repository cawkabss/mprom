{
  "name": "mprom-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "file-saver": "^1.3.3",
    "history": "^4.7.2",
    "material-ui": "^0.20.0",
    "material-ui-next": "^1.0.0-beta.30",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-form-validator-core": "^0.3.0",
    "react-material-ui-form-validator": "^1.0.7",
    "react-redux": "^5.0.6",
    "react-responsive-carousel": "^3.1.33",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.0.17",
    "react-sticky": "^6.0.1",
    "react-tap-event-plugin": "^3.0.2",
    "recharts": "^1.0.0-beta.9",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build-prod": "npm install --only=dev && npm install && npm run build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-dev": "npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:3001",
  "devDependencies": {
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2"
  }
}
